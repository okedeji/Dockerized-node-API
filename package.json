{
  "name": "ecommerce-shop",
  "version": "1.0.0",
  "description": "e-commerce application",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec babel-node ./src/index.js",
    "test": "env NODE_ENV=test mocha --require @babel/register --timeout 10000",
    "build": "env NODE_ENV=production babel src -d dist --copy-files",
    "start": "env NODE_ENV=production node dist/index.js",
    "test:watch": "env NODE_ENV=test mocha --require @babel/register --watch ./test ./ --timeout 10000"
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "express",
    "caching"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "express-winston": "^3.1.0",
    "fancy-log": "^1.3.3",
    "g": "^2.0.1",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.3.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
